{
  "name": "hexo-site",
  "version": "0.0.0",
  "private": true,
  "hexo": {
    "version": "3.3.9"
  },
  "dependencies": {
    "hexo": "^3.3.9",
    "hexo-codepen": "^0.1.1",
    "hexo-deployer-git": "^0.3.1",
    "hexo-generator-archive": "^0.1.4",
    "hexo-generator-category": "^0.1.3",
    "hexo-generator-index": "^0.2.0",
    "hexo-generator-search": "^2.1.1",
    "hexo-generator-tag": "^0.2.0",
    "hexo-processor-static": "^1.0.0",
    "hexo-renderer-coffeescript": "^0.2.0",
    "hexo-renderer-ejs": "^0.3.0",
    "hexo-renderer-marked": "^0.3.0",
    "hexo-renderer-pug": "0.0.5",
    "hexo-renderer-stylus": "^0.3.1",
    "hexo-server": "^0.2.2",
    "hexo-tag-bootstrap": "0.0.8",
    "hexo-tag-fontawesome": "^1.0.0",
    "hexo-toc-ext": "^1.0.5",
    "jquery": "^3.2.1",
    "jsdom": "^11.3.0"
  },
  "scripts": {
    "start": "node node_modules/hexo/bin/hexo server -w",
    "n-post": "hexo n demo-post",
    "p-post": "hexo p demo-post",
    "g": "node node_modules/hexo/bin/hexo clean && node node_modules/hexo/bin/hexo g",
    "deploy": "node node_modules/hexo/bin/hexo clean && node node_modules/hexo/bin/hexo d --generate",
    "git-pull": "git pull origin gh-pages",
    "git-up": "git add . && git commit -m \"update on `date +\"%Y/%m/%d %H:%M:%S\"`\" && git push origin gh-pages"
  }
}
